<style>
  .card-sup {
    background-color: rgb(216, 189, 189);
  }

  body {
    background-color: #1a1a1a;
    margin: 0;
  }

  .div-fix {
    background-color: #f86e04;
    /*width: 100%;*/

    position: static;
    margin-top: 53px;
    margin-bottom: 5.5%;
  }

  /*Lançamentos*/


  .lancamentos {
    padding: 20px;
    margin: 30px 0px 100px;
    width: 100%;
  }

  .lancamentos h2 {
    text-align: center;
    color: rgb(255, 255, 255);
    font-family: 'Montserrat', sans-serif;
    font-style: italic;
    font-weight: 100;
    font-size: 350%;
    margin-bottom: 15px;
  }

  .lancamentos hr {
    color: rgb(255, 255, 255);
    width: 40%;
    margin: 0px auto 60px auto;
  }

  .produtos {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    /* centraliza no eixo horizontal */
    align-items: center;
    /* centraliza no eixo vertical */
  }

  .produto {
    width: 30%;
    margin: 10px;
    padding: 10px;
    border: 1px solid rgb(141, 141, 141);
    border-radius: 5px;
    box-shadow: 2px 2px 5px rgba(0, 0, 0, 0.1);

  }

  .produto img {
    width: 100%;
    height: auto;
    border-radius: 5px;
  }

  .produto h3 {
    font-size: 20px;
    margin: 15px 0 0 2px;
    color: rgb(212, 212, 212);
    font-family: 'Montserrat', sans-serif;
    font-weight: 600;
  }

  .produto p {
    font-size: 14px;
    margin: 3px 0 5px;
    margin-left: 4px;
    color: rgb(156, 156, 156);
    font-family: 'Montserrat', sans-serif;
    font-weight: bold;
  }

  .produto p:last-child {
    font-weight: bold;
    color: #f66f6f;
  }

  /*banner*/

  .banner {
    background-image: url(/images/AAAAAAA\ copy.jpg);
    background-size: cover;
    background-position: center;
    height: 500px;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
  }

  .containera {
    width: 100%;
    max-width: 1600px;
    margin: 0 auto;
    padding: 0 20px;
  }

  .banner-content {
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    justify-content: center;
  }


  .banner-image {
    flex-basis: 50%;
    text-align: center;
  }

  .banner-image img {
    max-width: 50%;
    max-height: 50%;
    height: auto;
  }

  .banner-text {
    flex-basis: 50%;
    text-align: center;
    border-left: 1px solid rgb(97, 97, 97);
    padding-left: 50px;
  }

  .banner-text h1 {
    font-size: 40px;
    font-weight: 300;
    margin-bottom: 20px;
    color: rgb(212, 212, 212);
    font-family: 'Montserrat', sans-serif;
  }

  .banner-text p {
    font-size: 24px;
    line-height: 1.5;
    color: rgb(212, 212, 212);
    font-family: 'Montserrat', sans-serif;
    font-weight: 200;
  }

  /*Mapa*/

  .mapa {
    width: 90%;
    height: 390px;
    display: block;

  }

  /*Sobre nós*/

  .sobre-nos p {
    font-size: 24px;
    line-height: 1.5;
    max-width: 1000px;
    margin: 20px auto;
    font-family: 'Montserrat', sans-serif;
    font-weight: 400;
    color: rgb(212, 212, 212);
    text-align: justify;
  }

  .sobre-nos {
    margin: 60px 0px;

    text-align: center;
  }

  .sobre {
    background-color: #f86e04;
    font-size: 50px;
    margin: 150px 0 50px;
    font-family: 'Montserrat', sans-serif;
    font-weight: 600;
    color: rgb(240, 240, 240);
    text-align: center;
    padding: 50px;
  }

  /*Footer*/

  footer {
    background-color: #f86e04;
    width: 100%;
    padding: 10%;
    clear: none;
  }

  .footer_c {
    background-color: #f86e04;
    margin-bottom: -100px;
    clear: left;
  }

  /* Estilos padrão */

  /* Adicione a seguinte regra de media query para telas menores que 768px */
  @media only screen and (max-width: 768px) {
    .lancamentos h2 {
      font-size: 250%;
    }

    .produto {
      width: 45%;
    }

    .produto h3 {
      font-size: 18px;
    }

    .produto p {
      font-size: 12px;
    }

    .banner-image {
      flex-basis: 100%;
      margin-bottom: 20px;
    }

    .banner-text {
      flex-basis: 100%;
      border-left: none;
      padding-left: 0;
    }

    .banner-text h1 {
      font-size: 32px;
    }

    .banner-text p {
      font-size: 18px;
    }

    .mapa {
      height: 300px;
    }
  }

  /* Adicione a seguinte regra de media query para telas menores que 480px */
  @media only screen and (max-width: 480px) {
    .lancamentos h2 {
      font-size: 200%;
    }

    .produto {
      width: 80%;
      margin-right: 8%;
    }

    .produto h3 {
      font-size: 16px;
    }

    .produto p {
      font-size: 10px;
    }

    .banner-text h1 {
      font-size: 24px;
    }

    .banner-text p {
      font-size: 16px;
    }

    .mapa {
      height: 200px;
    }
  }

  /* Estilos para telas menores */
  @media only screen and (max-width: 600px) {
    .sobre-nos {
      padding: 50px;
    }
  }

  @media (max-width: 768px) {
    .mapa {
      display: flex;
      justify-content: center;
      margin-left: 5%;
    }
  }
</style>

